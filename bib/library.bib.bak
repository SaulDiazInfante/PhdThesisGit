@Article{Mller-Gronbach2012,
  Title                    = {Derandomization of the Euler scheme for scalar stochastic differential equations},
  Author                   = {Mller-Gronbach, Thomas and Ritter, Klaus and Yaroslavtseva, Larisa},
  Year                     = {2012},
  Doi                      = {10.1016/j.jco.2011.12.001},
  ISSN                     = {0885064X},
  Number                   = {2},
  Pages                    = {139--153},
  Volume                   = {28},

  Abstract                 = {Consider a scalar stochastic differential equation with solution process X. We present a deterministic algorithm to approximate the marginal distribution of X at t=1 by a discrete distribution, and hereby we get a deterministic quadrature rule for expectations E(f(X(1))). The construction of the algorithm is based on derandomization of the Euler scheme. We provide a worst case analysis for the computational cost and the error, assuming that the coefficients of the equation have bounded derivatives up to order four and that the derivatives of f are polynomially bounded up to order four. In terms of the computational cost the error is almost of the order 23, if the diffusion coefficient is bounded away from zero, and in general we almost achieve the order 12. ?? 2011 Elsevier Inc. All rights reserved.},
  File                     = {:home/saul/Documents/Mendeley Desktop/Mller-Gronbach, Ritter, Yaroslavtseva/Journal of Complexity/Mller-Gronbach, Ritter, Yaroslavtseva - 2012 - Derandomization of the Euler scheme for scalar stochastic differential equations.pdf:pdf},
  Journal                  = {Journal of Complexity},
  Keywords                 = {Constructive quantization,Derandomization of Euler scheme,Quadrature of stochastic differential equations,Sparse Markov chain,Worst case analysis}
}

