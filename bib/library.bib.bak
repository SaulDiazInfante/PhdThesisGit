@Article{Hutzenthaler2011b,
  Title                    = {Divergence of the multilevel Monte Carlo Euler method for nonlinear stochastic differential equations},
  Author                   = {Hutzenthaler, Martin and Jentzen, Arnulf and Kloeden, Peter E.},
  Year                     = {2013},
  Doi                      = {10.1214/12-AAP890},
  Eprint                   = {1105.0226},
  ISSN                     = {1050-5164},
  Month                    = oct,
  Number                   = {5},
  Pages                    = {1913--1966},
  Url                      = {http://arxiv.org/abs/1105.0226 http://projecteuclid.org/euclid.aoap/1377696302},
  Volume                   = {23},

  Abstract                 = {One of the most promising methods for simulating moments or other expectations of functionals of the solution process of a stochastic differential equation is the multilevel Monte Carlo Euler method in [Giles, M. B., Multilevel Monte Carlo Path Simulation, Operations Research 56 (2008), no. 3, 606-617]. This method has been shown to converge if the coefficients of the SDE are globally Lipschitz continuous. In this article we establish that the multilevel Monte Carlo Euler method does, in general, not converge if the coefficients of the SDE are not globally Lipschitz continuous. More precisely, the main result of this article proves divergence of the multilevel Monte Carlo Euler method for a family of SDEs with superlinearly growing and globally one-sided Lipschitz continuous drift coefficients. This divergence result reveals that the multilevel Monte Carlo Euler method often produces completely wrong values in the case of SDEs with superlinearly growing coefficients. Additionally, this divergence of the new multilevel Monte Carlo Euler method is in fundamental contrast to the convergence of the classical Monte Carlo Euler method for such SDEs. Nonetheless, the multilevel Monte Carlo method combined with a strongly convergent numerical approximation method does converge. In particular, we show that the multilevel Monte Carlo Euler method combined with a tamed Euler method converges for such SDEs.},
  Archiveprefix            = {arXiv},
  Arxivid                  = {1105.0226},
  File                     = {:home/saul/Documents/Mendeley Desktop/Hutzenthaler, Jentzen, Kloeden/The Annals of Applied Probability/Hutzenthaler, Jentzen, Kloeden - 2013 - Divergence of the multilevel Monte Carlo Euler method for nonlinear stochastic differential equa.pdf:pdf},
  Journal                  = {The Annals of Applied Probability},
  Keywords                 = {and phrases,nonglobally lip-,nonlinear stochastic differential equations,rare events}
}

