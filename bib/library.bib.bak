@Article{Mao2007a,
  Title                    = {Exponential stability of equidistant Euler–Maruyama approximations of stochastic differential delay equations},
  Author                   = {Mao, Xuerong},
  Year                     = {2007},
  Doi                      = {10.1016/j.cam.2005.11.035},
  ISSN                     = {03770427},
  Month                    = mar,
  Number                   = {1},
  Pages                    = {297--316},
  Url                      = {http://linkinghub.elsevier.com/retrieve/pii/S0377042706000173},
  Volume                   = {200},

  Abstract                 = {Our aim is to study under what conditions the exact and numerical solution (based on equidistant nonrandom partitions of integration time-intervals) to a stochastic differential delay equation (SDDE) share the property of mean-square exponential stability. Our approach is trying to avoid the use of Lyapunov functions or functionals.We show that under a global Lipschitz assumption an SDDE is exponentially stable in mean square if and only if for some sufficiently small stepsizethe Euler–Maruyama (EM) method is exponentially stable in mean square. We then replace the global Lipschitz condition with a finite-time convergence condition and establish the same “if and only if” result. The important feature of this result is that it transfers the asymptotic problem into a finite-time convergence problem. Replacing the exact andEMnumerical solution with stochastic processes, we also discuss whether a family of stochastic processes share the stability property. This new approach allows us to discuss (i) whether a family of SDDEs share the stability property, and (ii) whether an SDDE with variable time lag shares stability property with the modifiedEMmethod. As another application of this general approach we consider a linear SDDE with variable time lag and establish an “if and only if” result. It should also be mentioned that the questions addressed, results proved, as well as style of analysis borrow heavily from [14] but the computations involved to cope with time delay are nontrivial},
  File                     = {:home/saul/Documents/Mendeley Desktop/Mao/Journal of Computational and Applied Mathematics/Mao - 2007 - Exponential stability of equidistant Euler–Maruyama approximations of stochastic differential delay equations.pdf:pdf},
  Journal                  = {Journal of Computational and Applied Mathematics},
  Keywords                 = {brownian motion,euler,exponential stability,it\^{o},maruyama,mean-square stability,s formula,s method,stochastic flow}
}

