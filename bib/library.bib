journal = {SIAM Journal on Numerical Analysis},
keywords = {060658138,10,1137,60h10,65c30,ams subject classifications,backward euler,doi,euler,growth condition,implicit,linear,lyapunov exponent,maruyama,one-sided lipschitz condition,stochastic theta method,verry important examples for stability},
