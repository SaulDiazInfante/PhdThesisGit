journal = {Applied Numerical Mathematics},
keywords = {Constant elasticity of variance,Displaced diffusion,Eternal life span,Forward rate model,Libor market models,Mean reverting,Milstein schemes,Positivity,Stochastic differential equation,Structure-preserving},
