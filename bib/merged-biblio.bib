  issn		= {00104655},
  journal	= {Computer Physics Communications},
  keywords	= {Advanced computer architecture,CUDA,Diffusion,Graphics
		  processing unit,Josephson
		  junction,Kuramoto,NVIDIA,Numerical integration,Stochastic
		  differential equation,Tesla},
  month		= mar,
  pages		= {183--188},
  title		= {{Accelerating numerical solution of stochastic
		  differential equations with CUDA}},
