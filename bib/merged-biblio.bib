  file		= {:home/saul/Documents/Mendeley Desktop/Lamba, Mattingly,
		  Stuart/IMA Journal of Numerical Analysis/Lamba, Mattingly,
		  Stuart - 2007 - An adaptive Euler-Maruyama scheme for SDEs
		  Convergence and stability.pdf:pdf},
  issn		= {02724979},
  journal	= {IMA Journal of Numerical Analysis},
  keywords	= {Adaptive time
		  discretization,Convergence,Ergodicity,Exponential moment
		  bounds,Stability,Stochastic differential equations},
  pages		= {479--506},
  primaryclass	= {math},
  title		= {{An adaptive Euler-Maruyama scheme for SDEs: Convergence
		  and stability}},
  volume	= {27},
  year		= {2007}
}

