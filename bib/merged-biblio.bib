@Article{	  hofmann2000,
  abstract	= {We study the pathwise (strong) approximation of scalar
		  stochastic differential equations with respect to the
		  global error in the L2-norm. For equations with additive
		  noise we establish a sharp lower error bound in the class
		  of arbitrary methods that use a fixed number of
		  observations of the driving Brownian motion. As a
		  consequence, higher order methods do not exist if the
		  global error is analyzed. We introduce an adaptive
		  step-size control for the Euler scheme which performs
		  asymptotically optimally. In particular, the new method is
		  more efficient than an equidistant discretization. This
		  superiority is confirmed in simulation experiments for
		  equations with additive noise, as well as for general
		  scalar equations.},
  author	= {Hofmann, Norbert and Uller-gronbach, Thomas M and Ritter,
		  Klaus},
  file		= {:home/saul/Documents/Mendeley Desktop/Hofmann,
		  Uller-gronbach, Ritter/MATHEMATICS OF COMPUTATION/Hofmann,
