@InProceedings{	  delahoz2012a,
  abstract	= {The investigation of interacting population models has
		  long been and will continue to be one of the dominant
		  subjects in mathematical ecology; moreover, the persistence
		  and extinction of these models is one of the most
		  interesting and important topics, because it provides
		  insight into their behavior. The mean extinction-time
		  depends on the initial population size and satisfies the
		  backward Kolmogorov differential equation, a linear
		  second-order partial differential equation with variable
		  coefficients; hence, finding analytical solutions poses
		  severe problems, except in a few simple cases, so we can
		  only compute numerical approximations (an idea already
		  mentioned in Sharp and Allen (1998) [1]). In this paper, we
		  study a stochastic Lotka-Volterra model (Allen, 2007) [2,
		  p. 149]; we prove the nonnegative character of its
		  solutions for the corresponding backward Kolmogorov
		  differential equation; we propose a finite element method,
		  whose Matlab code is offered in the Appendix; and, finally,
		  we make a direct comparison between predictions and
		  numerical simulations of stochastic differential equations
		  (SDEs). ?? 2011 Elsevier Inc. All rights reserved.},
  author	= {{De La Hoz}, F. and Vadillo, F.},
  booktitle	= {Applied Mathematics and Computation},
  doi		= {10.1016/j.amc.2012.05.060},
  file		= {:home/saul/Documents/Mendeley Desktop/De La Hoz,
		  Vadillo/Applied Mathematics and Computation/De La Hoz,
		  Vadillo - 2012 - A mean extinction-time estimate for a
		  stochastic Lotka-Volterra predator-prey model(2).pdf:pdf},
  issn		= {00963003},
  keywords	= {Extinction-time,Finite elements method,Population
		  models,Stochastic differential equations},
  number	= {1},
  pages		= {170--179},
  title		= {{A mean extinction-time estimate for a stochastic
		  Lotka-Volterra predator-prey model}},
  volume	= {219},
  year		= {2012}
}

@Article{	  delay2005,
  abstract	= {Since the first attempts some 20 yr ago in the field of
		  hydrology, random walk (RW) particle tracking as applied to
		  solute transport has experienced profound changes. Concepts
		  and mathematical techniques have improved to the point that
		  numerically difficult problems (e.g., advection-dominated
		  transport in highly heterogeneous media, or reactive
		  transport) are now much easier to address. Random walk has
		  never been widely used for multiphase flow, probably
		  because numerical dispersion is not a major problem for
		  modeling exercises at large scales. However, vadose zone
		  hydrologic studies often point out very strong variations
		  in fluid velocity over relatively short distances. Random
		  walk methods may be well suited for such studies, a
		  possibility which motivated us to write this review. We
		  first give a comprehensive discussion of the theoretical
		  context of the method. The Fokker-Planck-Kolmogorov
		  equation (FPKE) is established for solute transport, as
		  well as the ordinary Langevin equation and its
		  simplifications for transport of small particles (e.g.,
		  colloids). Next, numerical methods are developed for the
		  motion of particles in space. An important section is
		  subsequently dedicated to recent RW concepts in the time
		  domain, and to their application to anomalous (non-Fickian)
		  transport and inverse problems. Adaptations of RW to
		  transport with solute-solid reactions are also provided, as
		  well as several numerical recipes for resolving a few
		  computational difficulties with the RW method. We purposely
		  did not include any comparisons with Eulerian and
		  Lagrangian approaches. These approaches are discussed at
		  length in several references cited in this review. We note,
		  however, that today's computing capabilities provide new
		  incentives to using RW methods for problems where Eulerian
		  methods are potentially unstable or hampered by numerical
		  diffusion.},
  author	= {Delay, F. and Ackerer, P. and Danquigny, C.},
  doi		= {10.2136/vzj2004.0125},
  file		= {:home/saul/Documents/Mendeley Desktop/Delay, Ackerer,
		  Danquigny/Vadose Zone Journal/Delay, Ackerer, Danquigny -
		  2005 - Simulating Solute Transport in Porous or Fractured
		  Formations Using Random Walk Particle Tracking.pdf:pdf},
  issn		= {15391663},
  journal	= {Vadose Zone Journal},
  number	= {2},
  pages		= {360--379},
  publisher	= {Soil Science Society of America},
  title		= {{Simulating Solute Transport in Porous or Fractured
		  Formations Using Random Walk Particle Tracking}},
  url		= {https://www.soils.org/publications/vzj/abstracts/4/2/0360}
		  ,
  volume	= {4},
  year		= {2005}
}

@Article{	  demir2000,
  abstract	= {Phase noise is a topic of theoretical and practical
		  interest in$\backslash$nelectronic circuits, as well as in
		  other fields, such as optics.$\backslash$nAlthough progress
		  has been made in understanding the phenomenon,
		  there$\backslash$nstill remain significant gaps, both in
		  its fundamental theory and in$\backslash$nnumerical
		  techniques for its characterization. In this paper, we
		  develop$\backslash$na solid foundation for phase noise that
		  is valid for any oscillator,$\backslash$nregardless of
		  operating mechanism. We establish novel results about
		  the$\backslash$ndynamics of stable nonlinear oscillators in
		  the presence of$\backslash$nperturbations, both
		  deterministic and random. We obtain an
		  exact$\backslash$nnonlinear equation for phase error, which
		  we solve without$\backslash$napproximations for random
		  perturbations. This leads us to a
		  precise$\backslash$ncharacterization of timing jitter and
		  spectral dispersion, for computing$\backslash$nof which we
		  have developed efficient numerical methods. We
		  demonstrate$\backslash$nour techniques on a variety of
		  practical electrical oscillators and$\backslash$nobtain
		  good matches with measurements, even at frequencies close
		  to the$\backslash$ncarrier, where previous techniques break
		  down. Our methods are more than$\backslash$nthree orders of
		  magnitude faster than the brute-force Monte
		  Carlo$\backslash$napproach, which is the only previously
		  available technique that can$\backslash$npredict phase noise correctly},
