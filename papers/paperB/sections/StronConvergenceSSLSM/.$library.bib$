@Article{NORBERTHOFMANNTHOMASMULLER-GRONBACH2012,
  Title                    = {{Optimal Approximation of Stochastic Differential Equations by Adaptive Step-Size Control}},
  Author                   = {Hofmann, Norbert and M\"{u}ller-Gronbach, Thomas and Ritter, Klaus},
  Journal                  = {Mathematics of Computation},
  Year                     = {2012},
  Number                   = {231},
  Pages                    = {1017--1034},
  Volume                   = {69},

  Abstract                 = {We study the pathwise (strong) approximation of scalar stochastic differential equations with respect to the global error in the L2-norm. For equations with additive noise we establish a sharp lower error bound in the class of arbitrary methods that use a fixed number of observations of the driving Brownian motion. As a consequence, higher order methods do not exist if the global error is analyzed. We introduce an adaptive step-size control for the Euler scheme which performs asymptotically optimally. In particular, the new method is more efficient than an equidistant discretization. This superiority is confirmed in simulation experiments for equations with additive noise, as well as for general scalar equations.},
  File                     = {:home/saul/Documents/Mendeley Desktop/Hofmann, M\"{u}ller-Gronbach, Ritter/Mathematics of Computation/Hofmann, M\"{u}ller-Gronbach, Ritter - 2012 - Optimal Approximation of Stochastic Differential Equations by Adaptive Step-Size Control.pdf:pdf}
}

