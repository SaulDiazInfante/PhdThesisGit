journal = {Journal of Differential Equations},
keywords = {and phrases,bifurcations,concentration of measure,dynamic,fast systems,first-exit times,for sdes,geometric singular perturbation theory,invariant manifolds,running head,singular perturbations,slow,stochastic differential equations},
