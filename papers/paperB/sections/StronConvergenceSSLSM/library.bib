issn = {03784754},
journal = {Mathematics and Computers in Simulation},
keywords = {Bacterial Physiological Phenomena,Biodegradation,Biofilms,Biofilms: growth \& development,Biological,Chemical,Chemical: metabolism,Environmental,Mathematics,Models,Nonlinear Dynamics,Water Pollutants},
month = nov,
number = {1-4},
pages = {105--113},
pmid = {17883224},
title = {{Combined nonstandard numerical methods for ODEs with polynomial right-hand sides}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17883224 http://linkinghub.elsevier.com/retrieve/pii/S0378475406001789},
